      {
        "id": "OrderManagement",
        "name": "OrderManagement",
        "body": "export default {\n  // Inicializar store\n  init: async () => {\n    if (!appsmith.store.orderProducts) {\n      await storeValue('orderProducts', []);\n    }\n    if (!appsmith.store.contacts) {\n      await storeValue('contacts', [\n        { id: 1, name: 'Juan Pérez', email: 'juan.perez@example.com', phone: '+34 600 111 222', address: 'Calle Mayor 1, Madrid' },\n        { id: 2, name: 'María García', email: 'maria.garcia@example.com', phone: '+34 600 222 333', address: 'Av. Diagonal 100, Barcelona' },\n        { id: 3, name: 'Carlos López', email: 'carlos.lopez@example.com', phone: '+34 600 333 444', address: 'Plaza España 5, Sevilla' },\n        { id: 4, name: 'Ana Martínez', email: 'ana.martinez@example.com', phone: '+34 600 444 555', address: 'Gran Vía 20, Valencia' },\n        { id: 5, name: 'Pedro Sánchez', email: 'pedro.sanchez@example.com', phone: '+34 600 555 666', address: 'Paseo Recoletos 15, Madrid' }\n      ]);\n    }\n  },\n  \n  // Gestión de contactos\n  addContact: async (name, email, phone, address) => {\n    if (!name || !email) {\n      await showAlert('Nombre y email son obligatorios', 'error');\n      return false;\n    }\n    \n    const contacts = appsmith.store.contacts || [];\n    const newContact = {\n      id: Date.now(),\n      name: name,\n      email: email,\n      phone: phone || '',\n      address: address || ''\n    };\n    \n    await storeValue('contacts', [...contacts, newContact]);\n    await showAlert(`Contacto \"${name}\" agregado exitosamente`, 'success');\n    return true;\n  },\n  \n  getContactOptions: () => {\n    const contacts = appsmith.store.contacts || [];\n    return contacts.map(c => ({\n      label: `${c.name} (${c.email})`,\n      value: c.id.toString()\n    }));\n  },\n  \n  getSelectedContactInfo: () => {\n    const contacts = appsmith.store.contacts || [];\n    const selectedId = ContactSelector.selectedOptionValue;\n    if (!selectedId) return null;\n    \n    return contacts.find(c => c.id.toString() === selectedId);\n  },\n  \n  // Gestión de productos en el pedido\n  addProductToOrder: async () => {\n    const products = appsmith.store.orderProducts || [];\n    const newProduct = {\n      id: Date.now(),\n      name: '',\n      quantity: 1,\n      unitPrice: 0,\n      subtotal: 0\n    };\n    await storeValue('orderProducts', [...products, newProduct]);\n  },\n  \n  removeProduct: async (productId) => {\n    const products = appsmith.store.orderProducts || [];\n    const filtered = products.filter(p => p.id !== productId);\n    await storeValue('orderProducts', filtered);\n  },\n  \n  updateProduct: async (productId, field, value) => {\n    const products = appsmith.store.orderProducts || [];\n    const updated = products.map(p => {\n      if (p.id === productId) {\n        const updatedProduct = { ...p, [field]: value };\n        // Recalcular subtotal si cambia cantidad o precio\n        if (field === 'quantity' || field === 'unitPrice') {\n          updatedProduct.subtotal = (updatedProduct.quantity || 0) * (updatedProduct.unitPrice || 0);\n        }\n        return updatedProduct;\n      }\n      return p;\n    });\n    await storeValue('orderProducts', updated);\n  },\n  \n  calculateTotal: () => {\n    const products = appsmith.store.orderProducts || [];\n    return products.reduce((sum, p) => sum + (p.subtotal || 0), 0);\n  },\n  \n  // Guardar pedido\n  saveOrder: async () => {\n    const contactId = ContactSelector.selectedOptionValue;\n    const orderNumber = OrderNumber.text;\n    const orderDate = OrderDate.selectedDate;\n    const status = OrderStatus.selectedOptionValue;\n    const notes = OrderNotes.text;\n    const products = appsmith.store.orderProducts || [];\n    \n    // Validaciones\n    if (!contactId) {\n      await showAlert('Debe seleccionar un contacto', 'error');\n      return;\n    }\n    \n    if (!orderNumber) {\n      await showAlert('Debe ingresar el número de pedido', 'error');\n      return;\n    }\n    \n    if (!orderDate) {\n      await showAlert('Debe seleccionar la fecha del pedido', 'error');\n      return;\n    }\n    \n    if (products.length === 0) {\n      await showAlert('Debe agregar al menos un producto', 'error');\n      return;\n    }\n    \n    // Validar que todos los productos estén completos\n    const invalidProducts = products.filter(p => !p.name || p.quantity <= 0 || p.unitPrice <= 0);\n    if (invalidProducts.length > 0) {\n      await showAlert('Todos los productos deben tener nombre, cantidad y precio válidos', 'error');\n      return;\n    }\n    \n    const contactInfo = OrderManagement.getSelectedContactInfo();\n    const total = OrderManagement.calculateTotal();\n    \n    const order = {\n      orderNumber: orderNumber,\n      date: orderDate,\n      status: status,\n      contact: contactInfo,\n      products: products,\n      notes: notes,\n      total: total,\n      createdAt: new Date().toISOString()\n    };\n    \n    // Aquí se guardaría en la base de datos\n    console.log('Pedido a guardar:', order);\n    \n    await showAlert(`Pedido ${orderNumber} guardado exitosamente. Total: ${total.toFixed(2)} €`, 'success');\n    await OrderManagement.clearForm();\n  },\n  \n  clearForm: async () => {\n    await storeValue('orderProducts', []);\n    await resetWidget('OrderNumber', true);\n    await resetWidget('OrderDate', true);\n    await resetWidget('OrderStatus', true);\n    await resetWidget('OrderNotes', true);\n    await resetWidget('ContactSelector', true);\n  }\n}",
        "pluginType": "JS"
      }
